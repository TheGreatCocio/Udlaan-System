<Window x:Class="UdlaanSystem.UIUserNote"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UdlaanSystem"
        mc:Ignorable="d"
        Title="UIUserNote" Height="300" Width="500.409">
    <Grid>
        <TextBox x:Name="textBoxUserNote" HorizontalAlignment="Left" Height="193" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="472" TextChanged="TextBoxUserNote_TextChanged"/>
        <Label x:Name="label" Content="Her kan i skrive en note om brugeren der vil poppe op fremover når i scanner personen" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonConfirmUserNote" Content="OK" HorizontalAlignment="Left" Margin="407,239,0,0" VerticalAlignment="Top" Width="75" Click="ButtonConfirmUserNote_Click"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="10,234,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.266,-0.098" Height="25"/>

    </Grid>
</Window>
