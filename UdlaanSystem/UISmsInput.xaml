<Window x:Class="UdlaanSystem.UISmsInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UdlaanSystem"
        mc:Ignorable="d"
        Title="UISmsInput" Height="132.173" Width="234.836">
    <Grid>
        <Label x:Name="label" Content="Indtast Koden Der Er Sendt Med SMS" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxSmsInput" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="207" PreviewTextInput="NumberValidationTextBox"/>
        <Button x:Name="buttonDone" Content="Udfør" HorizontalAlignment="Left" Margin="115,69,0,0" VerticalAlignment="Top" Width="102" Click="buttonDone_Click"/>
        <Button x:Name="buttonCancel" Content="Afbryd" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top" Width="100" Click="buttonCancel_Click"/>

    </Grid>
</Window>
