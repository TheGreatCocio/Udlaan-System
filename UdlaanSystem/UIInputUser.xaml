<Window x:Class="UdlaanSystem.UIInputUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UdlaanSystem"
        mc:Ignorable="d"
        Title="Opret/Redigere Bruger" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBoxZbcName" HorizontalAlignment="Left" Height="23" Margin="27,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" TextChanged="TextBoxZbcName_TextChanged"/>
        <TextBox x:Name="textBoxUserMifare" HorizontalAlignment="Left" Height="23" Margin="27,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" TextChanged="TextBoxUserMifare_TextChanged"/>
        <TextBox x:Name="textBoxPhoneNumber" HorizontalAlignment="Left" Height="23" Margin="27,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" TextChanged="TextBoxPhoneNumber_TextChanged"/>
        <Label x:Name="LabelFName" Content="Fornavn" HorizontalAlignment="Left" Margin="27,176,0,0" VerticalAlignment="Top" Width="56"/>
        <Label x:Name="LabelLName" Content="Efternavn" HorizontalAlignment="Left" Margin="27,207,0,0" VerticalAlignment="Top" Width="62"/>
        <Button x:Name="ButtonCreateOrUpdate" Content="Button" HorizontalAlignment="Left" Margin="191,233,0,0" VerticalAlignment="Top" Width="84" Click="ButtonCreateOrUpdate_Click" IsEnabled="False"/>
        <Label x:Name="LabelSearch" Content="Søg(Zbc Navn)" HorizontalAlignment="Left" Margin="23,5,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelUserMifare" Content="Mifare" HorizontalAlignment="Left" Margin="23,59,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelPhoneNumber" Content="Mobil Nr." HorizontalAlignment="Left" Margin="23,113,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelFNameResult" Content="" HorizontalAlignment="Left" Margin="88,176,0,0" VerticalAlignment="Top" Width="178"/>
        <Label x:Name="LabelLNameResult" Content="" HorizontalAlignment="Left" Margin="94,207,0,0" VerticalAlignment="Top" Width="172"/>

    </Grid>
</Window>
